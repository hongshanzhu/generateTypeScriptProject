{
  "name": "nodejsProject",
  "version": "1.0.0",
  "description": "generate nodejs Project",
  "license": "UNLICENSED",
  "scripts": {
    "clean": "shx rm -rf lib build reports",
    "format": "tsfmt -r",
    "compile": "tsc --pretty && echo TypeScript compile passed || echo TypeScript compile has error",
    "lint": "tslint --force --project tsconfig.json",
    "build": "npm run clean && npm run format && npm run compile && npm run lint",
    "testall": "npm run build && _mocha -c build/test/**/test-*.js",
    "test": "run() { _mocha -c build/test/**/test-*$1*.js; }; run",
    "prepublish": "shx cp -R build/src lib",
    "ci": "npm run build && npm run ci:test && npm run ci:coverage && npm run ci:lint && npm run prepublish",
    "ci:test": "XUNIT_FILE=reports/xunit.xml istanbul cover _mocha 'build/test/**/test-*.js' -- --reporter xunit-file",
    "ci:coverage": "remap-istanbul -i reports/coverage/coverage-final.json -t cobertura -o reports/coverage/cobertura.xml",
    "ci:lint": "tslint --force -t pmd --project tsconfig.json > reports/pmd.xml",
    "start": "NODE_ENV=production node lib/index.js >> /var/log/nodejs.log 2>&1 &"
  },
  "main": "lib/index.js",
  "engines": {
    "node": "^6.5.0"
  },
  "dependencies": {
    "body-parser": "1.15.2",
    "express": "4.14.0",
    "request": "2.69.0",
    "soap": "0.16.0"
  },
  "devDependencies": {
    "@types/body-parser": "0.0.33",
    "@types/chai": "3.4.34",
    "@types/express": "4.0.33",
    "@types/joi": "9.0.32",
    "@types/lodash": "4.14.37",
    "@types/mocha": "2.2.32",
    "@types/nock": "8.0.33",
    "@types/oracledb": "1.11.33",
    "@types/pg": "6.1.33",
    "@types/promisify-supertest": "1.0.31",
    "@types/request": "0.0.31",
    "@types/sinon": "1.16.31",
    "@types/sinon-chai": "2.7.27",
    "@types/soap": "0.0.30",
    "@types/supertest": "1.1.31",
    "@types/typescript": "0.4.29",
    "chai": "*",
    "mocha": "3.1.2",
    "nock": "^8.1.0",
    "promisify-supertest": "^1.0.0",
    "remap-istanbul": "0.7.0",
    "scripty": "1.7.0",
    "shx": "0.1.4",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "supertest": "^2.0.0",
    "trace": "2.3.3",
    "tslint": "3.15.1",
    "tslint-eslint-rules": "2.1.0",
    "typescript": "2.0.3",
    "typescript-formatter": "3.1.0",
    "xunit-file": "1.0.0"
  }
}
